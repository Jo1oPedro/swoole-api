{
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Database\\": "database",
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "framework"
        }
    ],
    "require-dev": {
        "swoole/ide-helper": "~5.0.0"
    },
    "require": {
        "cascata/framework": "dev-main",
        "ext-pdo": "*",
        "ext-memcached": "*"
    },
    "scripts": {
        "create-migration": "vendor/bin/doctrine-migrations generate",
        "migrate": "vendor/bin/doctrine-migrations migrate"
    }
}
